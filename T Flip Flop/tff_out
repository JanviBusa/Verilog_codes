#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5611790737f0 .scope module, "tff_tb" "tff_tb" 2 24;
 .timescale 0 0;
v0x5611790869b0_0 .var "clk", 0 0;
v0x561179086a80_0 .net "q", 0 0, v0x56117903bba0_0;  1 drivers
v0x561179086b50_0 .var "t", 0 0;
S_0x561179073980 .scope module, "uut" "t_flip_flop" 2 30, 2 3 0, S_0x5611790737f0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "t";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /OUTPUT 1 "q";
v0x56117903b7b0_0 .net "clk", 0 0, v0x5611790869b0_0;  1 drivers
v0x56117903bba0_0 .var "q", 0 0;
v0x561179086890_0 .net "t", 0 0, v0x561179086b50_0;  1 drivers
E_0x56117903a480 .event posedge, v0x56117903b7b0_0;
    .scope S_0x561179073980;
T_0 ;
    %wait E_0x56117903a480;
    %load/vec4 v0x561179086890_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x56117903bba0_0;
    %inv;
    %assign/vec4 v0x56117903bba0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x56117903bba0_0;
    %assign/vec4 v0x56117903bba0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x561179073980;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56117903bba0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x5611790737f0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5611790869b0_0, 0, 1;
T_2.0 ;
    %delay 5, 0;
    %load/vec4 v0x5611790869b0_0;
    %inv;
    %store/vec4 v0x5611790869b0_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x5611790737f0;
T_3 ;
    %vpi_call 2 38 "$monitor", $time, " clock=%b, T=%b, Q=%b", v0x5611790869b0_0, v0x561179086b50_0, v0x561179086a80_0 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5611790737f0;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561179086b50_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x561179086b50_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561179086b50_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x561179086b50_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 46 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x5611790737f0;
T_5 ;
    %vpi_call 2 50 "$dumpfile", "tff.dump" {0 0 0};
    %vpi_call 2 51 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5611790737f0 {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "tff.v";
